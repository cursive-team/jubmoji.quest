{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "paths": {
      "@/*": [
        "./src/*"
      ],
    },
    "resolveJsonModule": true
  },
  "include": [
    "./next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "./node_modules"
  ]
}
