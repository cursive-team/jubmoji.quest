// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CardholderLog {
  id            Int      @id @default(autoincrement())
  signature     String   
  messageHash   String   
  publicKey     String   
  verified      Boolean  
  createdAt     DateTime @default(now())
}